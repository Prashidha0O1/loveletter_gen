{
  "expo": {
    "name": "loveletter_generator",
    "slug": "loveletter_generator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Cupid.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Cupid.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.drzoom.cupidquill"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Cupid.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e71b2faa-3589-48ad-8903-74c7af524a10"
      }
    }
  }
}
